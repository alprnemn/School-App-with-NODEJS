
<% if (typeof message != "undefined") { %>
 
    <div id="popup" class="p-4 md:p-5 text-center bg-blue-950 fixed top-[50%] left-[30%] md:left-[50%] rounded-3xl">
        <svg class="mx-auto mb-4 text-white w-12 h-12" aria-hidden="true" xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 20 20">
            <path stroke="currentColor" stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M10 11V6m0 8h.01M19 10a9 9 0 1 1-18 0 9 9 0 0 1 18 0Z"/>
        </svg>
        <h3 class="mb-5 text-lg font-normal text-<%= message.class %>-700"><%= message.text %></h3>
        <button type="button" id="popupbutton" class="text-white bg-blue-600  font-medium rounded-lg text-sm inline-flex items-center px-5 py-2.5 text-center">
            Confirm
        </button>
        
    </div>
    
<% } %>